generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Board {
    id String @id @default(uuid())
    title String 
    
    orgId String     

    imageId String
    imageThumbUrl String @db.Text
    imageFullUrl String @db.Text
    imageLinkHTML String @db.Text
    imageUsername String @db.Text
}


